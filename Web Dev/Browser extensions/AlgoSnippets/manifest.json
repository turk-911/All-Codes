{
  "manifest_version": 3,
  "name": "Algorithm Snippet Finder",
  "version": "1.0",
  "description": "Select an algorithm and get the corresponding code snippet.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "permissions": ["storage"]
}